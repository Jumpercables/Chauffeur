<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- =============================================================
      The JENKINS server configurations.    
    ================================================================== -->
    <add key="Chauffeur/Jenkins/Server" value="http://ap-tfs2010-p02:8080/"/>
    <add key="Chauffeur/Jenkins/User" value="kbaesler"/>
    <add key="Chauffeur/Jenkins/Token" value="586d09cecce248ef9ce9830331ded88a"/>
    <!-- =============================================================
      The PACKAGES installation and de-installation configurations.
    ================================================================== -->
    <add key="Chauffeur/Packages/DataDirectory" value=""/>
    <add key="Chauffeur/Packages/UninstallPropertyReferences" value="/quiet"/>
    <add key="Chauffeur/Packages/InstallPropertyReferences" value="/quiet"/>
    <!-- =============================================================
      The NOTIFICATION configurations.
   
      [*] {BUILD(NUMBER)}          : The unique number of the build. 
      [*] {BUILD(TIMESTAMP)}       : The date the build was created. 
      [*] {BUILD(DURATION)}        : The time it took for the build to complete. 
      [*] {BUILD(FULLDISPLAYNAME)} : The full name of the build.       
      [*] {BUILD(ID)}              : The unique identifier for the build. 
      [*] {BUILD(URL)}             : The url location of the build.       
      [*] {CHAUFFEUR(MACHINENAME)} : The name of the machine that host the service.
    ================================================================== -->
    <add key="Chauffeur/Notifications/Host" value="smtp.sempra.com"/>
    <add key="Chauffeur/Notifications/To" value="kbaesler@semprautilities.com"/>
    <add key="Chauffeur/Notifications/From" value="kbaesler@semprautilities.com"/>
    <add key="Chauffeur/Notifications/IsHtml" value="false"/>
    <add key="Chauffeur/Notifications/Subject" value="Build Installed: {BUILD(NUMBER)} on {CHAUFFEUR(MACHINENAME)}."/>
    <add key="Chauffeur/Notifications/Body" value="Team, 
         
         The latest installer for Desktop has been installed on the {CHAUFFEUR(MACHINENAME)} citrix server. 
         
         - Name:   {BUILD(FULLDISPLAYNAME)}
         - Number: {BUILD(NUMBER)}      
         - Paths:  {CHANGESET(AFFECTEDPATHS)}   
         "/>
  </appSettings>
  <system.diagnostics>
    <trace autoflush="true">
      <listeners>
        <add name="logListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="Chauffeur.log" />
        <add name="consoleListener" type="System.Diagnostics.ConsoleTraceListener"/>
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>